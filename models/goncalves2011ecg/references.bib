@article{goncalves2009ontological2,
	title        = {An ontological analysis of the electrocardiogram},
	author       = {Gon\c{c}alves, B.;zamborlini, V.;guizzardi, G.},
	year         = 2009,
	month        = {mar.},
	journal      = {Revista Eletr\^{o}nica de Comunica\c{c}\~{a}o, Informa\c{c}\~{a}o e Inova\c{c}\~{a}o em Sa\'{u}de},
	volume       = 3,
	number       = 1,
	pages        = {45--59},
	doi          = {10.3395/reciis.v3i1.819},
	url          = {https://brapci.inf.br/index.php/res/v/129335},
	language     = {pt-BR},
	abstract     = {A bioinform\'{a}tica tem sido um campo f\'{e}rtil para aplica\c{c}\~{a}o da disciplina de ontologia formal. A representa\c{c}\~{a}o com princ\'{\i}pios de entidades biom\'{e}dicas tem alimentado progressivamente a pesquisa biol\'{o}gica, com benef\'{\i}cios diretos que v\~{a}o da reformula\c{c}\~{a}o das terminologias m\'{e}dicas \`{a} introdu\c{c}\~{a}o de novas perspectivas para modelos aprimorados de Registros Eletr\^{o}nicos de Sa\'{u}de (RES). Este artigo apresenta uma an\'{a}lise ontol\'{o}gica do eletrocardiograma (ECG) independente de aplica\c{c}\~{a}o e fundamentada na Ontologia Fundamental Unificada. Com o objetivo de investigar o fen\^{o}meno subjacente a esse exame cardiol\'{o}gico, trabalhamos com os subdom\'{\i}nios da eletrofisiologia e anatomia do cora\c{c}\~{a}o humano. Ent\~{a}o delineamos uma Ontologia do ECG constru\'{\i}da sobre a Ontologia Relacional OBO (Open Biomedical Ontology). Al\'{e}m disso, a ontologia de dom\'{\i}nio esbo\c{c}ada aqui se inspira tanto no Modelo Fundamental de Anatomia como na Ontologia de Fun\c{c}\~{o}es proposta sob o amparo do programa de pesquisa em Ontologia Geral Formal (OGF)},
	author_keywords = {Ontologia biomedica\@pt-BR;Eletrocardiograma\@pt-BR;Eletrofisiologia cardiaca\@pt-BR;Biomedical ontology\@en;Electrocardiogram\@en;Heart electrophysiology\@en},
	source       = {Brapci}
}
