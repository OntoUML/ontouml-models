@phdthesis{albuquerque2011desenvolvimento,
	title        = {{Desenvolvimento de uma Ontologia de Dom{\'{i}}nio para Modelagem de Biodiversidade}},
	author       = {Albuquerque, Andr{\'{e}}a Corr{\^{e}}a Flores},
	year         = 2011,
	pages        = 147,
	abstract     = {A Conven{\c{c}}{\~{a}}o sobre Diversidade Biol{\'{o}}gica (Convention on Biological Diversity - CBD) reconhece que a perda da biodiversidade deve ser reduzida para promover a redu{\c{c}}{\~{a}}o da pobreza e beneficiar diretamente todas as formas de vida na Terra. Para tanto, devem-se considerar estrat{\'{e}}gias robustas e planos de a{\c{c}}{\~{a}}o baseados em conhecimento e no estado da arte da tecnologia. Neste contexto, temos dois cen{\'{a}}rios atuais: a Web como ambiente de dissemina{\c{c}}{\~{a}}o e a Web Sem{\^{a}}ntica para tratar a complexidade de dom{\'{i}}nios com grande expressividade sem{\^{a}}ntica, como o da biodiversidade. A pesquisa est{\'{a}} em andamento em universidades e organiza{\c{c}}{\~{o}}es cient{\'{i}}ficas objetivando o desenvolvimento da Web Sem{\^{a}}ntica como um recurso adicional associado {\`{a}} ontologia formal e {\`{a}} evas{\~{a}}o de problemas na aquisi{\c{c}}{\~{a}}o de conhecimento como a depend{\^{e}}ncia do expertise, o conhecimento t{\'{a}}cito, a disponibilidade dos peritos e a import{\^{a}}ncia do tempo ideal. Ontologia estrutura o processo de aquisi{\c{c}}{\~{a}}o de conhecimento para o prop{\'{o}}sito de compreens{\~{a}}o, entendimento de m{\'{a}}quinas port{\'{a}}teis e de extra{\c{c}}{\~{a}}o de conhecimento do ambiente da Web Sem{\^{a}}ntica. Estas tecnologias aplicadas ao dom{\'{i}}nio da biodiversidade s{\~{a}}o um recurso valioso para o planejamento estrat{\'{e}}gico do futuro da nossa regi{\~{a}}o e sua contribui{\c{c}}{\~{a}}o para o planeta.},
	keywords     = {Biodiversidade,Engenharia de Ontologia,Interoperabilidade,Ontologia de Dom{\'{i}}nio},
	institution  = {Universidade Federal do Amazonas},
	type         = {M.Sc. Dissertation}
}
