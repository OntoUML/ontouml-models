@prefix ontouml: <https://purl.org/ontouml-models/vocabulary/>.
@prefix dataset: <https://purl.org/ontouml-models/dataset/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix mod: <https://w3id.org/mod#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix dcat: <http://www.w3.org/ns/dcat#>.

<http://purl.org/ontouml-models/dataset/university-ontology/> a dcat:Dataset, mod:SemanticArtefact;
    dct:title "University Ontology";
    dct:subject "university";
    dcat:theme <http://id.loc.gov/authorities/classification/L>;
    skos:editorialNote "This model was done as an assignment for a course. The author required to remain anonymous. There is no information about the ontology's development year. The model was imported from the XMI provided at the source directory and adapted to correspond to the images there provided (e.g., some cardinalities were removed because were not present at the images).";
    dct:type ontouml:Domain;
    dct:language "en";
    mod:designedForTask ontouml:Learning;
    ontouml:context ontouml:Classroom;
    dct:conformsTo ontouml:OntoumlStyle.
