@prefix dash: <http://datashapes.org/dash#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix mod: <https://w3id.org/mod#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix dcat: <http://www.w3.org/ns/dcat#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix ontouml: <https://purl.org/ontouml-models/vocabulary/>.
@prefix osh: <http://purl.org/ontouml-models/shapes> .

osh:ModelCatalogShape rdf:type sh:NodeShape ;
    sh:targetClass dcat:Catalog ;
    sh:property [
        sh:path dct:identifier ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path dct:title ;
        sh:minCount 1 ;
        sh:or ([ sh:datatype rdf:langString ; ]
                [ sh:datatype xsd:string ; ]) ;
    ] ;
    sh:property [
        sh:path dct:alternative ;
        sh:or ([ sh:datatype rdf:langString ; ]
                [ sh:datatype xsd:string ; ]) ;
    ] ;
    sh:property [
        sh:path dct:description ;
        sh:or ([ sh:datatype rdf:langString ; ]
                [ sh:datatype xsd:string ; ]) ;
    ] ;
    sh:property [
        sh:path foaf:homepage ;
        sh:nodeKind sh:IRI ;
        sh:pattern "^https?://"
    ] ;
    sh:property [
        sh:path dcat:themeTaxonomy ;
        sh:minCount 1 ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path dct:bibliographicCitation ;
        sh:or ([ sh:datatype rdf:langString ; ]
                [ sh:datatype xsd:string ; ]) ;
    ] ;
    sh:property [
        sh:path dct:issued ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:or ([ sh:datatype xsd:dateTime ; ]
            [ sh:datatype xsd:date ; ]
            [ sh:datatype xsd:gYearMonth ; ]
            [ sh:datatype xsd:gYear ; ]) ;
    ] ;
    sh:property [
        sh:path dct:modified ;
        sh:maxCount 1 ;
        sh:or ([ sh:datatype xsd:dateTime ; ]
            [ sh:datatype xsd:date ; ]
            [ sh:datatype xsd:gYearMonth ; ]
            [ sh:datatype xsd:gYear ; ]) ;
    ] ;
    sh:property [
        sh:path dcat:contactPoint ;
        sh:qualifiedValueShape [
            sh:property [
                sh:path vcard:hasEmail ;
                sh:minCount 1 ;
            ] ;
            sh:property [
                sh:path vcard:fn ;
            ] ;
        ] ;
    ] ;
    sh:property [
        sh:path dct:publisher ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path dct:creator ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path dct:contributor ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path dct:accessRights ;
    ] ;
    sh:property [
        sh:path dct:license ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path dcat:distribution ;
        sh:nodeKind sh:IRI ;
        sh:class dcat:Distribution ;
    ] ;
    sh:property [
        sh:path dct:dataset ;
        sh:nodeKind sh:IRI ;
        sh:class mod:SemanticArtefact ;
    ] .