@prefix dcat:	<http://www.w3.org/ns/dcat#> .
@prefix dct:	<http://purl.org/dc/terms/> .
@prefix omt:	<https://w3id.org/ontouml-models/catalog-vocabulary#> .
@prefix owl:	<http://www.w3.org/2002/07/owl#> .
@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:	<http://www.w3.org/2004/02/skos/core#> .
@prefix vann:	<http://purl.org/vocab/vann/> .
@prefix xml:	<http://www.w3.org/XML/1998/namespace> .
@prefix xsd:	<http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/ontouml-models/catalog-vocabulary> a owl:Ontology ;
                                        dcat:keyword "OntoUML"@en, "Metamodel"@en, "Ontology-Driven Conceptual Modeling"@en ;
                                        dcat:landingPage <https://w3id.org/ontouml-models> ; 
                                        dct:contributor <https://orcid.org/0000-0003-3385-4769> ;
                                        dct:created "2022-04-03"^^xsd:date ;
                                        dct:creator <https://orcid.org/0000-0002-5385-5761> , <https://orcid.org/0000-0003-2736-7817> ;
                                        dct:description "Metadata entities and properties to describe models in the OntoUML/UFO Catalog."@en ;
                                        dct:identifier "https://w3id.org/ontouml-models/catalog-vocabulary"^^xsd:anyURI ;
                                        dct:isReferencedBy <https://doi.org/10.1007/978-3-031-17995-2_1> ;
                                        dct:license <https://creativecommons.org/licenses/by/4.0/legalcode> ;
                                        dct:modified "2023-03-03"^^xsd:date ;
                                        dct:publisher <https://www.utwente.nl/eemcs/scs/> ;
                                        dct:title "OntoUML/UFO Catalog Metadata Vocabulary"@en ;
                                        owl:versionInfo "1.0"@en ;
                                        owl:versionIRI <https://w3id.org/ontouml-models/catalog-vocabulary#1.0> ;
                                        skos:changeNote "dct:contributor, dcat:keyword, skos:changeNote, dct:isReferencedBy metadata added"@en ; 
                                        skos:changeNote "file scope re-defined to contain only the OntoUML catalog terms in OWL"@en ;
                                        vann:preferredNamespacePrefix "omt"@en ;
                                        vann:preferredNamespaceUri "https://w3id.org/ontouml-models/catalog-vocabulary#"^^xsd:anyURI .

omt:context a owl:ObjectProperty ;
            rdfs:range omt:OntologyDevelopmentContext .

omt:MetadataElement a owl:Class ;
                    rdfs:comment "An element used in the metadata description of OntoUML models."@en .

omt:OntologyDevelopmentContext a owl:Class ;
                                rdfs:subClassOf omt:MetadataElement .

omt:Classroom a omt:OntologyDevelopmentContext .
omt:Industry a omt:OntologyDevelopmentContext .
omt:Research a omt:OntologyDevelopmentContext .

omt:OntologyPurpose a owl:Class ;
                    rdfs:subClassOf omt:MetadataElement .

omt:ConceptualClarification a omt:OntologyPurpose .
omt:DataPublication a omt:OntologyPurpose .
omt:DecisionSupportSystem a omt:OntologyPurpose .
omt:Example a omt:OntologyPurpose .
omt:InformationRetrieval a omt:OntologyPurpose .
omt:Interoperability a omt:OntologyPurpose .
omt:LanguageEngineering a omt:OntologyPurpose .
omt:Learning a omt:OntologyPurpose .
omt:OntologicalAnalysis a omt:OntologyPurpose .
omt:SoftwareEngineering a omt:OntologyPurpose .

omt:OntologyType a owl:Class ;
                rdfs:subClassOf omt:MetadataElement .

omt:Application a omt:OntologyType .
omt:Core a omt:OntologyType .
omt:Domain a omt:OntologyType .

omt:OntologyRepresentationStyle a owl:Class ;
                                rdfs:subClassOf omt:MetadataElement .

omt:OntoumlStyle a omt:OntologyRepresentationStyle .
omt:UfoStyle a omt:OntologyRepresentationStyle .